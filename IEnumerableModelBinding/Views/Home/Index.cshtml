@model List<IEnumerableModelBinding.Controllers.Book>

@{    
    ViewBag.Title = "Home Page";
}

@using (Html.BeginForm())
{
    for (int i = 0; i < Model.Count; i++)
    {
        <b>@Model[i].Name</b>
        //Html.DisplayFor(m => m[i].Name, Model[i].Name);
        @Html.EditorFor(m => m[i].Quantity, Model[i].Quantity);
        @:<hr />
    }



    <input type="submit" value="Submit" />
    }


